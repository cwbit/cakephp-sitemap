{
    "name": "cwbit/cakephp-sitemap",
    "description": "Sitemap Generator plugin for CakePHP 3.x apps",
    "type": "cakephp-plugin",
    "license" : "MIT",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Sitemap\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sitemap\\Test\\": "tests"
        }
    }
}
